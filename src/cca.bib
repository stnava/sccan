@ARTICLE{Whitwell2007d,
  author = {Jennifer L Whitwell and Clifford R Jack and Joseph E Parisi and David
	S Knopman and Bradley F Boeve and Ronald C Petersen and Tanis J Ferman
	and Dennis W Dickson and Keith A Josephs},
  title = {Rates of cerebral atrophy differ in different degenerative pathologies.},
